@{
    ViewData["Title"] = "Home";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

@using System.Net.Sockets;
@using System.Net;
@{
    var uri = Url.Action();
    string localIP;
    using (Socket socket = new Socket(AddressFamily.InterNetwork, SocketType.Dgram, 0))
    {
        socket.Connect("8.8.8.8", 65530);
        IPEndPoint endPoint = socket.LocalEndPoint as IPEndPoint;
        localIP = endPoint.Address.ToString();
    }
    int port = 80;
    if(Context.Request.Host.Port != null){
        port = Context.Request.Host.Port.Value;
    }
}

<div class="text-center">
    <h1 class="display-4">Welcome to</h1>
    <img src="data:image/png;base64," />
    <h3>everything is working on @Context.Request.Scheme://@localIP:@port@Context.Request.PathBase.Value</h3>
    <h4><a href="mailto:h.abughalyoun@@gcesoft.com.jo">contact us</a></h4>
</div>
